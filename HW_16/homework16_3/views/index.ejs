<table class="table">
  <thead>
    <tr>
      <% if(Array.isArray(data)) {%>
        <% Object.keys(data[0]).map(key => {%>
        <th><%= key %></th>
        <% }) %>
      <%} else {%>
        <% Object.keys(data).map(key => {%>
        <th><%= key %></th>
        <% }) %>
      <%}%>
    </tr>
  </thead>
  <tbody>
    <% if(Array.isArray(data)) {%>
      <% data.map(userData => {%>
        <tr>
        <% Object.values(userData).map(row => { %>
          <td>
          <%= row %>
          </td>
        <%})%>
        </tr>
      <%})%>
    <%} else {%>
      <tr>
      <% Object.values(data).map(row => { %>
        <td>
        <%= row %>
        </td>
      <%})%>
      </tr>
    <% } %>
  </tbody>
</table>